{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
        "url": "results/default/uelocation.csv"
    },
    "transform": [],
    "layer": [
        {
            "mark": "circle",
            "encoding": {
                "x": {
                    "field": "X",
                    "type": "quantitative"
                },
                "y": {
                    "field": "Y",
                    "type": "quantitative"
                },
                "color": {
                    "field": "Hdirection",
                    "type": "nominal",
                    "title": "CellDirection"
                }
            }
        }
    ],
    "title": "UE locations in the network",
    "width": 600,
    "height": 600
}